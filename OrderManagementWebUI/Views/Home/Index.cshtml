        <div class="col-md-6"><input id="upCustId" placeholder="upCustId" type="text" name="fname"></div><div class="col-md-6"></div>
        <div class="col-md-6"><input id="uptxtName" placeholder="uptxtName" type="text" name="fname"></div><div class="col-md-6"></div>
        <div class="col-md-6"><input id="uptxtSurname" placeholder="uptxtSurname" type="text" name="fname"></div><div class="col-md-6"></div>
        <div class="col-md-6"><input id="uptxtAddress" placeholder="uptxtAddress" type="text" name="fname"></div><div class="col-md-6"></div>
